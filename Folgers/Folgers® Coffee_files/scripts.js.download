            calculateCups : function(serv) {
                var message = "";
                var ounces = 0;
                var gallons = 0;
                var quarts = 0;
                var wcups = 0;
                var tablespoons = 0;
                var cups = 0;
                var coffee;
                var water;

                if ((serv >= 1) && (serv <= 99999)) {

                    //Calculate Water
                    ounces = parseInt((+serv) * 6);
                    gallons = parseInt((+ounces) / 128);
                    ounces = parseInt(ounces - (+gallons * 128));
                    quarts = parseInt(quarts / 32);
                    ounces = parseInt(ounces - (+quarts * 32));
                    wcups = parseInt(ounces / 8);
                    ounces = parseInt(ounces - (+wcups * 8));
                    water = "";

                    //Serv: serv, Oz: ounces, Gallons: gallons, Quarts: quarts, Cups: wcups, Water: water

                    if (gallons > 0) {
                        if (gallons == 1) {
                            water = "<b>" + gallons + " gallon</b> ";
                        }
                        else {
                            water = "<b>" + gallons + " gallons</b> ";
                        }
                    }

                    if (quarts > 0) {
                        if (quarts == 1) {
                            water = water + "<b>" + quarts + " quart</b> ";
                        }
                        else {
                            water = water + "<b>" + quarts + " quarts</b> ";
                        }
                    }

                    if (wcups > 0) {
                        if (wcups == 1) {
                            water = water + "<b>" + wcups + " cup</b> ";
                        }
                        else {
                            water = water + "<b>" + wcups + " cups</b> ";
                        }
                    }

                    if (ounces > 0) {
                        if (ounces == 1) {
                            water = water + "<b>" + ounces + " ounce</b> ";
                        }
                        else {
                            water = water + "<b>" + ounces + " ounces</b> ";
                        }
                    }

                    water = water.trim();

                    tablespoons = serv;
                    cups = parseInt(tablespoons / 16);
                    tablespoons = parseInt(tablespoons - (cups * 16));
                    coffee = "";

                    if (cups > 0) {
                        if (cups == 1) {
                            coffee = "<b>" + cups + " cup</b>";
                        }
                        else {
                            coffee = "<b>" + cups + " cups</b>";
                        }
                    }

                    if (tablespoons > 0) {
                        if (coffee != "") {
                            coffee = coffee + " and ";
                        }
                        if (tablespoons == 1) {
                            coffee = coffee + "<b>" + tablespoons + " tablespoon</b>";
                        }
                        else {
                            coffee = coffee + "<b>" + tablespoons + " tablespoons</b>";
                        }
                    }

                    message = '<div class="measurements-container">';
