                    message += '<div class="item image-content brewed-coffee"><div class="text-content"><b>' + serv + ' serving' + (serv == 1 ? '' : 's') + '</b> of coffee</div></div>';
                    message += '<div class="item symbol"><div class="text-content">=</div></div>';
                    message += '<div class="item image-content ground-coffee"><div class="text-content">' + coffee + ' of ground coffee</div></div>';
                    message += '<div class="item symbol"><div class="text-content">+</div></div>';
                    message += '<div class="item image-content water"><div class="text-content">' + water + ' of water</div></div>';
                    message += '</div>';
                    message += '</div>';

                    site.howToMeasure.calculator.resultContainer.removeClass('no-result');

                    if (serv <= 4) {
                        site.howToMeasure.calculator.resultContainer.addClass('small');
                        site.howToMeasure.calculator.resultContainer.removeClass('large');
                    }

                    else {
                        site.howToMeasure.calculator.resultContainer.addClass('large');
                        site.howToMeasure.calculator.resultContainer.removeClass('small');
                    }
                }

                else {
                    if (isNaN(serv))
                        message = "<p><b>You must enter a number.</b></p>";
                    else if (serv > 99999)
                        message = "<p><b>Please enter a number between 1 and 99999.</b></p>";
                    else
                        message = "<p><b>Don't you want at least one cup of Folgers?</b></p>";

                    site.howToMeasure.calculator.resultContainer.addClass('no-result');
                }

                return message;
            },
            init : function(){
                $('.calculator #btnCalculate').on('click', this.buttonClickHandler);
                $('.calculator .reset, .result-container .reset').on('click', this.resetClickHandler);
                this.input.on('focus', this.inputFocusHandler);
                this.input.on('blur', this.inputBlurHandler);
                site.howToMeasure.document.on('keydown', this.documentKeyDownHandler);
            }

        },
        tabClickHandler : function(event) {
            if(!$(this).hasClass('current')) {
                site.howToMeasure.tab.removeClass('current');
                $(this).addClass('current');
                //site.howToMeasure.table.removeClass('current').filter('#' + $(this).data('chart').addClass('current'));
            }
        },

        //calculator functions
        init : function(){
            this.calculator.init();
            this.tab.on('click', this.tabClickHandler(event));
        }
    },
    howToVideos : {
        init : function() {
            /* How To Videos Bar (Coffee Basics Page) */
            /******************************************/
            $('.how-to-videos-bar .videos').slick({
                mobileFirst: true,
                arrows: false,
                infinite: false,
                slidesToShow: 1,
                slidesToScroll: 1,
                responsive: [
                    {
                        breakpoint: 768,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            arrows:true,
                            prevArrow: '<a href="#" class="folgers-prev"></a>',
                            nextArrow: '<a href="#" class="folgers-next"></a>',
                        }
                    },
                ]
            });
        }
    }
};


