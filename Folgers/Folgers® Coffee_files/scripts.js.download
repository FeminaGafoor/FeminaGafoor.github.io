/* FolgersCoffee Scripts */
var site = site || {};
site = {

    nav     : $('#main-nav'),
    footer  : $('#footer'),
    header  : $('#header'),
    content : $('#content'),
    html    : $('html'),

    mobile : window.matchMedia( "(max-width: 767px)" ),
    tablet : window.matchMedia( "(min-width: 768px) and (max-width: 1199px)" ),
    desktop : window.matchMedia( "(min-width: 992px)" ),

    customCheckboxes : {

        init : function() {
            $('.faux-checkbox-wrap').on( 'click', function(e) {
                e.preventDefault();
                var checkbox = $(this).find('.faux-checkbox');
                var input = $(this).find('input');
                if (checkbox.hasClass('checked')) {
                    checkbox.removeClass('checked');
                    input.prop('checked', false);
                    input.change();
                }
                else {
                    checkbox.addClass('checked');
                    input.prop('checked', true);
                    input.change();
                }
            });
        }
    },

    hero : {
        init : function() {
            if ($('.hero:not(.editmode) .item').length > 1) {
                $('.hero').slick({
                    autoplay:true,
                    autoplaySpeed:3000,
                    arrows: false,
                    dots: true,
                    infinite: true,
                    mobileFirst: true,
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    //prevArrow: '<button href="#" class="slick-arrow prev"><i class="arrow left"></i></button>',
                    //nextArrow: '<button href="#" class="slick-arrow next"><i class="arrow right"></i></button>',
                });
            }
            if ($('.jms-hero-v2').length) {

                var hero = $('.jms-hero-v2');

                // Instantiate Fallbacks
                var CMSautoplay = "false";
                var CMSautoplaySpeed = 3000;
                var CMStransition = "slide";

                // CMS Values (Data Attributes)
                CMSautoplay = $(hero).attr("data-autoplay");
