    productFilters : {

        init : function() {

            if ($('#product-filters').length) {

                $('#product-filters .filter .toggle').click( function(e) {
                    var self = $(this);
                    var target = $(this).attr('href');

                    if (self.hasClass('collapsed')) {
                        self.removeClass('collapsed');
                        site.productFilters.openFilter(target);

                        // When in tablet breakpoint(s), allow only one box to be open at once
                        if (site.tablet.matches) {
                            var allOtherFilters = $('#product-filters .filter .toggle').not(self);
                            allOtherFilters.addClass('collapsed');
                            allOtherFilters.parent().find('.shadow').removeClass('in').height('0px');
                            allOtherFilters.each(function () {
                                site.productFilters.closeFilter($(this).attr('href'));
                            });
                        }
                    }
                    else {
                        // Close the filter if the user clicks on it again
                        self.addClass('collapsed');
                        if (!site.mobile.matches) {
                            self.parent().find('.shadow').removeClass('in').height('0px');
                        }
                        site.productFilters.closeFilter(target);
                        $('body').unbind('.outsideFilters');
                    }

                    // Close all filters if the user clicks outside the filter area
                    if (!site.mobile.matches) {
                        $('body').bind('click.outside' + target, function (e) {
                            if ($(e.target).closest('#product-filters').length === 0) {
                                self.addClass('collapsed');
                                self.parent().find('.shadow').removeClass('in').height('0px');
                                site.productFilters.closeFilter(target);
                                $('body').unbind('.outside' + target);
                            }
                        });
                    }
                    return false;
                });

                // On mobile, scroll to the top of the filter list after collapsing it
                $('#product-filters').on("hidden.bs.collapse", function(e) {
                    'use strict';
                    var headerHeight = $('#header').outerHeight();
                    var target = $('#filter-bar');
                    e.preventDefault();
                    if (target.length) {
                        $('html, body').animate({
                            scrollTop: $(target).offset().top - headerHeight
                        }, 200);
                    }
                    return false;
                });
            }
        },

        openFilter : function(target) {
            $(target).addClass('in');
        },

        closeFilter : function(target) {
            $(target).removeClass('in');
            $(target).parent().find('.shadow').removeClass('in').height('0px');
        },

        setupFilterStates : function(filterGroup) {
            var productFilters = $('#product-filters');
            var buttonClass = productFilters.find('.filter .toggle');
            var panelClass  = productFilters.find('.filter .filter-choices');

            if (filterGroup) {
                filterGroup = '#product-filters .' + filterGroup + ' .toggle';
            }

            if ( site.mobile.matches ) {
                buttonClass.removeClass('collapsed');
                panelClass.addClass('in');
            }
            else {
                buttonClass.addClass('collapsed');
                panelClass.removeClass('in');
                $(filterGroup).click();
            }
        }
    },

